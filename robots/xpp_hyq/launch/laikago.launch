<launch>
 
  <!-- Upload URDF file to ros parameter server for rviz to find  -->
    <arg name="user_debug" default="false"/>
  <param name="robot_description" command="$(find xacro)/xacro '$(find xpp_hyq)/urdf/robot.xacro' --inorder 
 DEBUG:=$(arg user_debug)"/>
  
  <!-- Converts Cartesian state to joint state and publish TFs to rviz  --> 
  <node name="urdf_visualizer_laikago" pkg="xpp_hyq" type="urdf_visualizer_laikago" output="screen"/>
     
</launch>
